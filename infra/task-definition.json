{
    "containerDefinitions": [ 
       { 
         "environment": [
            {
               "name": "MONGODB_ADDON_URI",
               "value": "<MONGO_URI>"
            }
          ],
          "essential": true,
          "image": "335617278903.dkr.ecr.us-east-2.amazonaws.com/hiring-devops",
          "name": "hiring-devops",
          "portMappings": [ 
             { 
                "containerPort": 3000,
                "hostPort": 0,
                "protocol": "tcp"
             }
          ]
       }
    ],
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::335617278903:role/hiring-devops-task_execution_role",
    "family": "hiring-devops",
    "memory": "200",
    "runtimePlatform": {
         "operatingSystemFamily": "LINUX"
     }
 }